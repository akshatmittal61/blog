<%- include("partials/header") -%>

    <h1>Home Page</h1>
    <p>
        <%= startingContent %>
    </p>
    <div class="posts-container">
        <% for(let i=0;i<posts.length;++i){posts[i] %>
            <div class="post">
                <h3 class="post-title">
                    <%= posts[i].title %>
                </h3>
                <div class="post-content">
                    <%= posts[i].content %>
                </div>
            </div>
            <% } %>
    </div>

    <%- include("partials/footer") -%>