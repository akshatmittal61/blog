<%- include("partials/header") -%>

    <h1>Home Page</h1>
    <p>
        <%= startingContent %>
    </p>
    <div class="posts-container">
        <% posts.forEach((post)=>{ %>
            <div class="post">
                <h3 class="post-title">
                    <a href=<%= `/posts/${post.id}` %> >
                        <%= post.title %>
                    </a>
                </h3>
                <div class="post-content incomplete">
                    <%= post.content %>
                </div>
                <a class="post-link" target="_blank" href=<%=`/posts/${post.id}` %>>Read More</a>
            </div>
            <% }) %>
    </div>

    <%- include("partials/footer") -%>